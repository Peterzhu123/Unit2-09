public class Animal {
	private int _numberOfLegs;
	private String _color;

	public void playSound() {
		System.out.println("animal is making sound");
	}
	
	public Animal(int numberOfLegs, String color) {
		 _numberOfLegs = numberOfLegs;
		 _color = color;
	}

}
